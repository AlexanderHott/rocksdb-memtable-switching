{
  "version": "0.2.0",
  "configurations": [
  
    {
      "name": "Benchamrk",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/benchmark/cmake-build-debug/main",
      "args": [
        "benchmark-runs/static-memtables"

      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "outputCapture": "std"
    }
  ]
}